{
  "manifest_version": 2,

  "key": "",
  "version": "0.1.7.0",

  "name": "Hide Hot Network Question on Stack Exchange",
  "short_name": "Hide HNQs",
  "icons": {
    "128": "icon-128.png"
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": [
        "*://*.stackoverflow.com/*",
        "*://*.stackexchange.com/*",
        "*://*.superuser.com/*",
        "*://*.serverfault.com/*",
        "*://*.stackapps.com/*",
        "*://*.mathoverflow.com/*",
        "*://*.askubuntu.com/*"
      ],
      "css": [
        "content/content.css"
      ],
      "js": [
        "common/util.js",
        "content/content.js"
      ]
    }
  ],

  "background": {
    "scripts": [
      "common/util.js",
      "event/event.js"
    ],
    "persistent": false
  },

  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
